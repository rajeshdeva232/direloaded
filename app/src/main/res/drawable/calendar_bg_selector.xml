<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <item app:tsquare_state_range_middle="true">
        <color android:color="@color/appColor"/>
    </item>
    <item android:state_selected="true">
        <color android:color="@color/appColor"/>
    </item>
    <item android:state_pressed="true">
        <color android:color="@color/appColor"/>
    </item>
    <item app:tsquare_state_current_month="false">
        <color android:color="@color/colorWhite"/>
    </item>
    <item app:tsquare_state_highlighted="true">
        <color android:color="@color/appColor"/>
    </item>
    <item app:tsquare_state_today="true">
        <color android:color="@color/appColor"/>
    </item>
    <item>
        <color android:color="@color/colorWhite"/>
    </item>
</selector>
